    "version": "1.0.18",
    "author": "Robert Marshall (https://robertmarshall.dev)",
    "dependencies": {
        "@babel/runtime": "^7.12.5",
        "axios": ">=0.21.0",
        "oauth-signature": "^1.5.0"
    },
    "keywords": [
        "gatsby",
        "gatsby-source",
        "gatsby-plugin",
        "wordpress",
        "gravityforms"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/robmarshall/gatsby-source-gravityforms.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "babel-preset-env": "^1.7.0",
        "babel-preset-gatsby-package": "^0.8.0",
        "cross-env": "^7.0.3",
